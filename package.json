{
  "name": "ng2-sandbox",
  "version": "0.0.0",
  "description": "Angular2 webpack sandbox app with ng2-bootstrap and SASS",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": {
    "name": "Jcob Ross",
    "email": "jcob.iddle@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=5.10"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --display-modules --progress",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "del-cli ./dist",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "lint": "npm-run-all lint:js lint:ts",
    "lint:js": "eslint -c .eslintrc.yml *.js",
    "lint:ts": "tslint './src/**/*.ts'",
    "prebuild": "npm-run-all clean lint:ts test",
    "start": "npm run dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "typings": "typings install"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "base64id": "^0.1.0",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "lodash": "4.11.1",
    "moment": "^2.13.0",
    "ng2-bootstrap": "^1.0.12",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "underscore": "1.8.3",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.6",
    "bootstrap-sass": "^3.3.6",
    "conventional-changelog-cli": "~1.1.1",
    "copy-webpack-plugin": "~2.1.3",
    "cross-env": "~1.0.7",
    "css-loader": "~0.23.1",
    "del-cli": "~0.2.0",
    "eslint": "~2.8.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "~2.16.0",
    "imports-loader": "^0.6.5",
    "jasmine-core": "~2.4.1",
    "karma": "~0.13.22",
    "karma-chrome-launcher": "~0.2.3",
    "karma-jasmine": "~0.3.8",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.7.0",
    "node-sass": "~3.5.3",
    "npm-run-all": "~1.8.0",
    "postcss-loader": "~0.8.2",
    "raw-loader": "~0.5.1",
    "reflect-metadata": "0.1.2",
    "sass-loader": "~3.2.0",
    "style-loader": "~0.13.1",
    "ts-loader": "~0.8.2",
    "tslint": "~3.8.1",
    "typescript": "1.9.0-dev.20160409",
    "typings": "~0.8.1",
    "url-loader": "^0.5.7",
    "val-loader": "^0.5.0",
    "webpack": "~1.13.0",
    "webpack-dev-server": "~1.14.1"
  }
}
